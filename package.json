{
  "name": "aofl-starter",
  "version": "1.0.0-alpha.1",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "env:prod": "cross-env NODE_ENV=production",
    "env:dev": "cross-env NODE_ENV=development",
    "env:test": "cross-env NODE_ENV=test",
    "test": "npm run env:test -- npx aofl test",
    "prod": "npm run env:prod -- npx aofl build",
    "dev": "npm run env:dev -- npx aofl build",
    "start:dev": "npm run env:dev -- npx aofl serve",
    "start": "npm run env:prod -- npx aofl serve",
    "stats": "webpack --profile --config __config/webpack.prod.js --json > stats.json",
    "docs": "jsdoc -c __config/.jsdocrc.json",
    "i18n": "aofl i18n -p \"**/*.js\" -E \"**/*.spec.js\" -E \"**/node_modules_sourced\"",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@aofl/jsdoc-template": "3.4.0",
    "cross-env": "5.2.0",
    "eslint": "5.15.0",
    "eslint-config-aofl": "1.0.0-alpha.1",
    "jsdoc": "3.5.5"
  },
  "dependencies": {
    "@aofl/cli": "2.0.0",
    "@aofl/polyfill-service": "2.0.0",
    "@aofl/router": "2.0.0",
    "@aofl/server-environment": "2.0.0",
    "@aofl/store": "2.0.0",
    "@aofl/web-components": "2.0.0",
    "@babel/polyfill": "7.4.0",
    "@babel/runtime": "7.4.2",
    "@webcomponents/webcomponentsjs": "^2.2.1",
    "ajv": "^6.10.0",
    "isomorphic-fetch": "2.2.1",
    "lit-element": "2.1.0",
    "lit-html": "1.0.0",
    "svg-inline-loader": "^0.8.0"
  }
}
