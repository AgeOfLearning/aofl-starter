{
  "source": {
    "include": "js",
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|api-docs)"
  },
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "api-docs/",
    "recurse": true,
    "verbose": true
  },
  "tags": {
    "allowUnknownTags": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
